{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Settings": {
    "ApplicationName": "PlexRequests",
    "OverwriteSettings": "False"
  },
  "DatabaseSettings": {
    "Server": "localhost",
    "Database": "PlexRequests",
    "User": "plexadmin",
    "Password": "password"
  },
  "AuthenticationSettings": {
    "SecretKey": "<SECRETKEY>"
  },
  "TheMovieDbSettings": {
    "BaseUri": "https://api.themoviedb.org/3/",
    "ApiKey": "<APIKEY>"
  },
  "PlexSettings": {
    "DefaultLocalPort": 32400,
    "ConnectLocally": true,
    "PlexMediaItemUriFormat": "https://app.plex.tv/desktop#!/server/{0}/details?key=%2Flibrary%2Fmetadata%2F{1}"
  }
}
